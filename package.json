{
  "name": "redindi",
  "description": "A tool to make personal finance management easy, for Organizational Maniacs",
  "version": "0.1.0",
  "author": "Dario Varotto",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.8",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-jss": "^8.4.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
